{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run migration:run && npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:cd": "npm run migration:run && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
    "test": "cross-env NODE_ENV=automated_tests jest",
    "test:watch": "cross-env NODE_ENV=automated_tests jest --watch",
    "test:cov": "cross-env NODE_ENV=automated_tests jest --coverage --no-cache --runInBand",
    "test:debug": "cross-env NODE_ENV=automated_tests node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "schema:drop": "cross-env NODE_ENV=development npm run typeorm -- schema:drop -d ./src/config/orm.config.ts",
    "schema:sync": "cross-env NODE_ENV=development npm run typeorm -- schema:sync -d ./src/config/orm.config.ts",
    "migration:create": "cross-var npm run typeorm -- migration:create ./data/migrations/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm -- migration:generate -d ./src/config/orm.config.ts ./data/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d ./src/config/orm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/config/orm.config.ts",
    "fixtures": "cross-env NODE_ENV=development fixtures load ./data/fixture --sync --debug --dataSource=./src/config/orm.config.ts --require=ts-node/register --require=tsconfig-paths/register",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@casl/ability": "^6.7.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "better-sqlite3": "^9.6.0",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "husky": "^9.1.6",
    "jwks-rsa": "^3.1.0",
    "mysql2": "^3.11.3",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-fixtures-cli": "^4.0.0",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
